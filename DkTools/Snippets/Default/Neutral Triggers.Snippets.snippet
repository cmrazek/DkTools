<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Global Initialize</Title>
			<Description>Code snippet for Other Neutral Global Initialize</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>description</ID>
					<ToolTip>Description of the function's purpose.</ToolTip>
					<Default>WBDK fires this trigger as soon as the DLL containing it is loaded.</Default>
				</Literal>
			</Declarations>
			<Code Language="DK" Kind="classtrigger">
				<![CDATA[int globalinitialize()
description "$description$"
{
}
]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Global Terminate</Title>
			<Description>Code snippet for Neutral Trigger Global Terminate</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>description</ID>
					<ToolTip>Description of the function's purpose.</ToolTip>
					<Default>WBDK fires this trigger when the DLL containing it is terminated.</Default>
				</Literal>
			</Declarations>
			<Code Language="DK" Kind="classtrigger">
				<![CDATA[int globalterminate()
description "$description$"
{
}
]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Static Initialize</Title>
			<Description>Code snippet for Neutral Trigger Static Initialize</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>description</ID>
					<ToolTip>Description of the function's purpose.</ToolTip>
					<Default>WBDK fires this trigger as soon as the DLL containing it is referenced by the application.</Default>
				</Literal>
			</Declarations>
			<Code Language="DK" Kind="classtrigger">
				<![CDATA[int staticinitialize()
description "$description$"
{
}
]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Static Terminate</Title>
			<Description>Code snippet for Neutral Trigger Static Terminate</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>description</ID>
					<ToolTip>Description of the function's purpose.</ToolTip>
					<Default>WBDK fires this trigger when the DLL containing it is no longer referenced by the application.</Default>
				</Literal>
			</Declarations>
			<Code Language="DK" Kind="classtrigger">
				<![CDATA[int staticterminate()
description "$description$"
{
}
]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
